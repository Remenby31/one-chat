{
  "mcpServers": {
    "shadcn": {
      "command": "npx",
      "args": [
        "shadcn@latest",
        "mcp"
      ]
    },
    "assistant-ui": {
      "command": "npx",
      "args": ["-y", "@assistant-ui/mcp-docs-server"]
    },
    "electron": {
      "command": "node",
      "args": ["mcp-servers/electron-mcp-server/build/index.js"]
    }
  }
}
