{
  "name": "jarvis",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.cjs",
  "scripts": {
    "start": "npx cross-env NODE_ENV=development electron .",
    "dev": "vite",
    "build": "tsc -b && vite build && electron-builder",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "npx wait-on http://localhost:5173 -t 30000 && vite build --config electron.vite.config.ts && npx cross-env NODE_ENV=development electron .",
    "electron:dev": "npx concurrently -k \"npm run dev\" \"npm run electron\"",
    "electron:build": "npm run build && electron-builder",
    "dist": "electron-builder",
    "generate-icons": "node scripts/generate-icons.cjs && node scripts/generate-ico.cjs && node scripts/generate-icns.cjs"
  },
  "dependencies": {
